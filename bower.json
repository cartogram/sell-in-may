{
  "name": "latitude",
  "version": "0.0.1",
  "homepage": "https://github.com/cartogram/latitude",
  "authors": [
    "Matthew Seccafien <matt@studiocartogram.com>"
  ],
  "description": "The Latitude theme is a simply Shopify theme that should provide a modern 'base' for scaffolding shopify themess at Cartogram.",
  "moduleType": [
    "node"
  ],
  "main": [
    "assets/*",
    "config/*",
    "layout/*",
    "snippets/*",
    "templates/*"
  ],
  "keywords": [
    "shopify",
    "cartogram"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "modernizr": "~2.8.2",
    "jquery": "~2.1.1",
    "foundation": "~5.4.3",
    "angular": "~1.3.0-rc.0",
    "swiper": "~2.7.0",
    "instafeed.js": "~1.3.2",
    "jquery-instagram": "~0.3.1",
    "angular-waypoints": "~1.0.0",
    "lodash": "~2.4.1",
    "angular-animate": "~1.2.25",
    "jquery.lazyload": "~1.9.3"
  },
  "devDependencies": {},
  "resolutions": {
    "angular": "1.2.25"
  }
}
